<div class="container-fluid">
    <div class="row">
        <div class="col">
            <table class="table table-striped">
                <thead>
                <tr>
                    <td>Url</td>
                    <td>Result</td>
                    <td>Method</td>
                    <td>Remote Address</td>
                </tr>
                </thead>
                @foreach (var item in _entries)
                {
                    <tr>
                        <td>@item.Url</td>
                        <td>@item.Result</td>
                        <td>@item.Method</td>
                        <td>@item.RemoteAddress</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>